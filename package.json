    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.7.12",
    "nodemailer": "^6.4.2",
    "nodemon": "^2.0.1",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-linkedin-oauth2": "^2.0.0",
    "path-parser": "^6.1.0",
    "sendgrid": "^5.2.3",
    "stripe": "^7.14.0",
    "swagger-jsdoc": "^3.5.0",
    "swagger-ui-express": "^4.1.2"
  },
  "devDependencies": {
    "jest": "^24.9.0",
